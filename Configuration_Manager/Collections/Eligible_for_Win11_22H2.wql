SELECT SMS_R_SYSTEM.ResourceID,SMS_R_SYSTEM.ResourceType,SMS_R_SYSTEM.Name,SMS_R_SYSTEM.SMSUniqueIdentifier,SMS_R_SYSTEM.ResourceDomainORWorkgroup,SMS_R_SYSTEM.ClientS
FROM SMS_R_System 
INNER JOIN SMS_G_System_UPGRADE_EXPERIENCE_INDICATORS on SMS_G_System_UPGRADE_EXPERIENCE_INDICATORS.ResourceID = SMS_R_System.ResourceId
WHERE SMS_G_System_UPGRADE_EXPERIENCE_INDICATORS.UpgExProp = "Green" and SMS_G_System_UPGRADE_EXPERIENCE_INDICATORS.Version = "NI22H2" and SMS_G_System_UPGRADE_EXPERIENCE_INDICATORS.UpgExU = "Green"